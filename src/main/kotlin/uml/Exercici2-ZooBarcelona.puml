@startuml
'https://plantuml.com/class-diagram

class Animal {
    -- Atributs --
    - especie: String
    - edat: Int
    - nom: String
    - habitat: String
    -- Mètodes --
    + moure()
    + emetreSo()
    + menjar()
}

class Visitant {
    -- Atributs --
    - nom: String
    - edat: Int
    - visitats: List<Animal>
    -- Mètodes --
    + afegitAnimal(visitats: List<Animal>): List<Animal>
}

@enduml